{
  "name": "mySongs",
  "version": "0.5.0",
  "manifest_version": 2,
  "description": "EnyoJS-App for displaying OpenLyric lyrics and chords",
  "app": {
      "launch": {
          "local_path": "index.html"
      }
  },
  "icons": {
    "64": "assets/images/icon64.png",
    "256": "assets/images/icon256.png"
  },
  "permissions": [
    "unlimitedStorage",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
